            <tr>
                <th>Tag</th>
                <th>Name</th>
                <th>Description</th>
                <th>CreatedTime</th>
                <th>Action</th>
            </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/groupsettings.js"></script>
}