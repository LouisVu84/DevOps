		<Element Type="SqlProcedure" Name="[dbo].[usp_CreateUser]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
	SET @ResponseCode = 1
	IF EXISTS (SELECT 1 FROM dbo.Users WHERE Email = @Email)
		BEGIN
			SET @ResponseCode = -1 -- email existed
			RETURN
		END

	IF EXISTS (SELECT 1 FROM dbo.UsersLogin WHERE UserName = @UserName)
		BEGIN
			SET @ResponseCode = -2 -- username existed
			RETURN
		END

	INSERT dbo.Users(FirstName, LastName, Email) VALUES (@FirstName, @LastName, @Email)

	SET @ResponseCode = SCOPE_IDENTITY()

	INSERT dbo.UsersLogin(Id, UserName, Password) VALUES (@ResponseCode, @UserName, @Password)
END]]></Value>
			</Property>
